


type t = Number of int
       | Var of string
       | Fun of string * t
       | App of t * t
       | Error of string



(* Syntax.to_string : Syntax.t -> string *)
let rec to_string exp = match exp with
    Number (n) -> string_of_int n
  | Var (x) -> x
  | Fun (x, t) ->
        "(fun " ^ x ^ " -> " ^ to_string t ^ ")"
  | App (t1, t2) ->
    "(" ^ to_string t1 ^ "@ " ^ to_string t2 ^ ")"
  | Error (s) -> "Error"^s



(* Syntax.print : Syntax.t -> unit *)
let print exp =
  let str = to_string exp in
  print_string str
